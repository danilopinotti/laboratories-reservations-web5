
<p id="notice"><%= notice %></p>
<h1>Listing Reserves</h1>

<table>
  <thead>
    <tr>
            <th>Laboratory</th>
            <th>User</th>
            <th>Begin at</th>
            <th>End at</th>
            <th>Day</th>
            <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reserves.each do |reserf| %>
    <tr>
            <td><%= reserf.laboratory %></td>
            <td><%= reserf.user %></td>
            <td><%= reserf.begin_at_id %></td>
            <td><%= reserf.end_at_id %></td>
            <td><%= reserf.day %></td>
            <td><%= link_to 'Show', reserf, {:class => "waves-effect waves-light btn light-green lighten-1"} %></td>
      <td><%= link_to 'Edit', edit_reserf_path(reserf), {:class => "waves-effect waves-light btn blue accent-3"} %></td>
      <td><%= link_to 'Destroy', reserf, method: :delete, data: { confirm: 'Are you sure?' }, :class => "waves-effect waves-light btn red darken-1" %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserve', new_reserf_path, {:class => "waves-effect waves-light btn blue darken-1"} %>
